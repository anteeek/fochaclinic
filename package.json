{
  "name": "astro",
  "version": "1.0.0",
  "description": "A simple static website with Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "build": "tailwindcss -i ./src/input.css -o ./src/dist/output.css",
    "watch": "tailwindcss -i ./src/input.css -o ./src/dist/output.css --watch",
    "start": "npx serve src",
    "publish": "npm run build && cp -rf src/* docs/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}
